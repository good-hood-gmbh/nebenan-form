{
  "name": "nebenan-form",
  "description": "React form components",
  "author": "Nebenan.de <max@nebenan.de>",
  "license": "SEE LICENSE IN LICENSE FILE",
  "readmeFilename": "README.md",
  "homepage": "https://nebenan.de/",
  "repository": "good-hood-gmbh/nebenan-form",
  "bugs": "https://github.com/good-hood-gmbh/nebenan-form/issues",
  "version": "0.1.0",
  "engines": {
    "node": "^8.x.x"
  },
  "scripts": {
    "clean": "rm -rf ./compiled/ ./lib/ style.css || exit 1",
    "build:babel": "NODE_ENV=production babel src --out-dir lib",
    "build:styles": "stylus index.styl -I node_modules -o style.css",
    "prepublishOnly": "npm run clean && npm run build:styles & npm run build:babel",
    "lint": "eslint --ext .es,.js ./",
    "lint:tofile": "eslint --ext .es,.js --no-color -o ./eslint-report.log ./",
    "test": "env TZ='Europe/Berlin' mocha \"test/**/*.js\"",
    "test:tofile": "mocha --no-colors -R spec \"test/**/*.js\" 2>&1 | tee test-report.log"
  },
  "devDependencies": {
    "nebenan-ui-kit": "^1.0.2",

    "lodash": "^4.17.4",
    "classnames": "^2.2.5",
    "react": "^16.x.x",
    "prop-types": "^15.6.x",
    "nib": "^1.1.2",

    "babel-cli": "6.26.0",
    "babel-register": "6.26.0",
    "babel-core": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-stage-3": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-plugin-transform-node-env-inline": "0.2.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.10",
    "stylus": "0.54.5",
    "eslint": "4.11.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-react": "7.5.1",
    "eslint-config-airbnb": "16.1.0",
    "stylint": "1.5.9",
    "stylint-stylish": "1.4.0",
    "stylint-json-reporter": "0.3.1",
    "mocha": "4.0.1",
    "chai": "4.1.2"
  },
  "peerDependencies": {
    "lodash": "^4.x.x",
    "classnames": "^2.2.5",
    "react": "^16.x.x",
    "prop-types": "^15.6.x"
  }
}
