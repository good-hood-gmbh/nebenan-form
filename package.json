{
  "name": "nebenan-form",
  "description": "React form components",
  "author": "Nebenan.de <max@nebenan.de>",
  "license": "SEE LICENSE IN LICENSE FILE",
  "readmeFilename": "README.md",
  "homepage": "https://nebenan.de/",
  "repository": "good-hood-gmbh/nebenan-form",
  "bugs": "https://github.com/good-hood-gmbh/nebenan-form/issues",
  "version": "0.1.0",
  "engines": {
    "node": "^8.x.x"
  },
  "scripts": {
    "clean": "rm -rf ./lib/ style.css || exit 1",
    "build:babel": "NODE_ENV=production babel src --out-dir lib",
    "build:styles": "stylus index.styl -I node_modules -o style.css",
    "preview:clean": "npm run clean && rm -rf ./preview/public || exit 1",
    "preview:babel": "mkdir -p ./preview/public && npm run build:babel && NODE_ENV=production watchify ./preview/app.es -v --extension .es -o ./preview/public/script.js -t [ babelify --extensions .es ]",
    "preview:styles": "mkdir -p ./preview/public && npm run build:styles && stylus ./preview/index.styl -I node_modules --include-css -w -o ./preview/public/style.css",
    "preview:server": "nodemon -e js,es --ignore test/ --ignore src/ --ignore preview/public/ --ignore node_modules ./preview/server.js",
    "watch": "npm run preview:clean && npm run preview:styles & npm run preview:babel",
    "prepublishOnly": "npm run clean && npm run build:styles & npm run build:babel",
    "lint": "eslint --ext .es,.js ./",
    "lint:tofile": "eslint --ext .es,.js --no-color -o ./eslint-report.log ./",
    "test": "env TZ='Europe/Berlin' mocha \"test/**/*.js\"",
    "test:tofile": "mocha --no-colors -R spec \"test/**/*.js\" 2>&1 | tee test-report.log",
    "start": "npm run preview:server"
  },
  "devDependencies": {
    "nebenan-ui-kit": "^1.2.1",
    "normalize.css": "^7.0.0",
    "lodash": "^4.17.4",
    "classnames": "^2.2.5",
    "react": "^16.x.x",
    "prop-types": "^15.6.x",
    "string-validate": "^1.x.x",
    "react-router": "3.2.0",
    "express": "4.16.2",
    "react-dom": "16.2.0",
    "morgan": "1.9.0",
    "serve-static": "1.13.1",
    "errorhandler": "1.5.0",
    "nib": "^1.1.2",
    "babel-cli": "6.26.0",
    "babel-register": "6.26.0",
    "babel-core": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-stage-3": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-plugin-transform-node-env-inline": "0.2.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.12",
    "watchify": "3.9.0",
    "babelify": "8.0.0",
    "stylus": "0.54.5",
    "nodemon": "1.14.7",
    "eslint": "4.14.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.5.1",
    "eslint-config-airbnb": "16.1.0",
    "stylint": "1.5.9",
    "stylint-stylish": "1.4.0",
    "stylint-json-reporter": "0.3.1",
    "mocha": "4.1.0",
    "chai": "4.1.2"
  },
  "peerDependencies": {
    "lodash": "^4.x.x",
    "classnames": "^2.2.5",
    "react": "^16.x.x",
    "prop-types": "^15.6.x",
    "string-validate": "^1.x.x"
  },
  "dependencies": {}
}
